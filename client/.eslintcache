: ', resp.data);\n            });\n        });\n    }\n    \n    search(searchTerm) {\n        \n        return new Promise((resolve, reject) => {\n\n            // const encodedSearchTerm = encodeURIComponent(searchTerm);\n\n            const config = {\n                headers: {\n                    'Authorization': 'Bearer 12345',\n                }\n            }\n            axios.get(`${apiEndpoint}/api/search/${searchTerm}`, config).then((resp) => {\n                resolve(resp.data);\n            });\n        });\n    }\n\n    getWatchlist(userID) {\n\n        return new Promise((resolve, reject) => {\n\n            const config = {\n                headers: {\n                    'Authorization': 'Bearer 12345',\n                }\n            }\n            axios.get(`${apiEndpoint}/api/watchlists/${userID}`, config).then((resp) => {\n                resolve(resp.data);\n            })\n        });\n    }\n\n    addItemToWatchlist(item) {\n\n        \n        return new Promise((resolve, reject) => {\n            // axios.post(`${apiEndpoint}/api/watchlists/create`, { 'item': item }).then(() => {\n            //     console.log(\"doe from here\")\n            // })\n\n            axios.patch(`${apiEndpoint}/api/watchlists`, { 'userID': '123456789', item: item }).then(() => {\n                console.log(\"doe from here\")\n            })\n        });\n    }   \n}","/Users/kyleplump/Projects/React/couchq/client/src/constants.js",[],"/Users/kyleplump/Projects/React/couchq/client/src/hooks/useViewportSize.js",[],["113","114"],"/Users/kyleplump/Projects/React/couchq/client/src/components/tabs.jsx",[],"/Users/kyleplump/Projects/React/couchq/client/src/components/search.jsx",[],"/Users/kyleplump/Projects/React/couchq/client/src/views/watchlist.jsx",["115","116"],"import { React, useState, useEffect } from 'react';\n// import MediaCard from '../components/mediaCard';\nimport WatchlistItem from '../components/watchlistItem';\nimport { Box, SimpleGrid } from '@chakra-ui/react';\nimport { APIService } from '../services/APIService';\nimport { connect } from 'react-redux';\nimport { cacheWatchlist } from '../state/actionCreators';\n\n\nfunction Watchlist(props) {\n\n    const [ gridColumns, setGridColumns ] = useState(1);\n    let api = null;\n\n    useEffect(() => {\n\n        api = new APIService();\n\n        if(props.watchlist.length === 0) {\n            api.getWatchlist('123456789').then((data) => {\n                props.cacheWatchlist(data);\n            });\n        }\n    }, []);\n\n    return (\n        <Box p={3}>\n            <SimpleGrid columns={gridColumns} spacing=\"15px\" role=\"list\">\n                {/* <MediaCard />\n                <MediaCard />\n                <MediaCard /> */}\n                {props.watchlist.map((item) => (\n                    <WatchlistItem item={item} key={item.id} />\n                ))}\n            </SimpleGrid>\n        </Box>\n    );\n}\n\nconst mapStateToProps = (state) => {\n\n    const { watchlist } = state;\n\n    return {\n        watchlist: watchlist.watchlist\n    }\n}\n\nconst mapDispatchToProps = {\n    cacheWatchlist\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Watchlist);","/Users/kyleplump/Projects/React/couchq/client/src/components/mediaCard.jsx",[],"/Users/kyleplump/Projects/React/couchq/client/src/components/header.jsx",[],"/Users/kyleplump/Projects/React/couchq/client/src/components/watchlistItem.jsx",[],"/Users/kyleplump/Projects/React/couchq/client/src/components/locationBadges/netflixBadge.jsx",[],["117","118"],"/Users/kyleplump/Projects/React/couchq/client/src/components/locationBadges/huluBadge.jsx",[],"/Users/kyleplump/Projects/React/couchq/client/src/views/searchResults.jsx",["119"],"import MediaCard from '../components/mediaCard';\nimport { SimpleGrid } from '@chakra-ui/react';\nimport { useHistory } from 'react-router-dom';\nimport { connect } from 'react-redux';\nimport { setActiveMediaItem } from '../state/actionCreators';\nimport { APIService } from '../services/APIService';\nimport { useEffect } from 'react';\n\nfunction SearchResults(props) {\n\n    const history = useHistory();\n    let api = null;\n\n    useEffect(() => {\n        api = new APIService();\n    }, [])\n\n    function viewResult(item) {\n\n        props.setActiveMediaItem(item.id, item);\n\n        history.push(`/view-details/${item.id}`);\n    }\n\n    function addResult(item) {\n\n        api.addItemToWatchlist(item);\n    }   \n\n    return (\n        <SimpleGrid columns={1} px={3} spacing={8}>\n            {\n                props.items.map((item, index) => {\n                    return <MediaCard clicked={() => viewResult(item)} onAdd={() => addResult(item)} \n                                key={index} img={item.picture} title={item.name} blurb={item.details.Plot} />\n                })\n            }\n        </SimpleGrid>\n    );\n}\n\nconst mapDispatchToProps = { setActiveMediaItem };\n\nexport default connect(null, mapDispatchToProps)(SearchResults);\n","/Users/kyleplump/Projects/React/couchq/client/src/state/store.js",[],"/Users/kyleplump/Projects/React/couchq/client/src/state/reducers/rootReducer.js",[],"/Users/kyleplump/Projects/React/couchq/client/src/state/reducers/mediaItem.js",["120"],"import { SET_ACTIVE_MEDIA_ITEM } from '../actionTypes';\n\nconst initialState = {\n    mediaItemID: null,\n    mediaItem: null\n}\n\nexport default function (state = initialState, action) {\n\n    switch(action.type) {\n\n        case SET_ACTIVE_MEDIA_ITEM: \n            const { itemID, item } = action.payload;\n            return {\n                ...state,\n                mediaItemID: itemID,\n                mediaItem: item\n            };\n\n        default: \n            return state;\n    }\n}","/Users/kyleplump/Projects/React/couchq/client/src/state/actionTypes.js",[],"/Users/kyleplump/Projects/React/couchq/client/src/state/actionCreators.js",[],"/Users/kyleplump/Projects/React/couchq/client/src/views/mediaDetail